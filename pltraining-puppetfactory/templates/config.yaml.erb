---
<% if @plugins -%>
:plugins:
<% @plugins.each do |plugin| -%>
  - :<%= plugin %>
<% end -%>
<% end -%>
<% if @usergroups -%>
:usergroups:
<% @usergroups.each do |group| -%>
  - <%= group %>
<% end -%>
<% end -%>
<% if @auth_info -%>
:auth_info:
<% @auth_info.each do |key, value| -%>
  :<%= key %>: <%= value %>
<% end -%>
<% end -%>
<% if @repomodel -%>
:repomodel: :<%= @repomodel %>
<% end -%>
<% if @modulepath -%>
:modulepath: :<%= @modulepath %>
<% end -%>
<% if @real_gitserver -%>
:gitserver: <%= @real_gitserver %>
<% end -%>
<% ['puppet', 'confdir', 'codedir', 'environments', 'root', 'logfile',
    'templatedir', 'port', 'bind', 'user', 'password', 'session', 'master',
    'usersuffix', 'autosign', 'puppetcode', 'gituser', 'githubtoken', 'controlrepo',
    'prefix', 'classifier', 'dashboard_path', 'dashboard_interval', 'container',
    'docker_group', 'docker_ip', 'privileged', 'hooks_path' ].each do |param| -%>
<% value = instance_variable_get("@#{param}") -%>
<% next unless value -%>
:<%= param %>: <%= value %>
<% end -%>
